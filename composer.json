{
    "name": "ezsystems/ezrecommendation-client",
    "description": "HTTP Communication Client between Recommendation engine and eZ Platform",
    "license": "GPL-2.0",
    "type": "ezplatform-bundle",
    "authors": [
        {
            "name": "eZ Systems",
            "email": "dev-team@ez.no"
        }
    ],
    "require": {
        "ezsystems/ezpublish-kernel": "~7.5.1",
        "ezsystems/doctrine-dbal-schema": "~0.1.1",
        "guzzlehttp/guzzle": "~6.0",
        "symfony/symfony": "^3.4.17",
        "webmozart/assert": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "ezsystems/ezplatform-code-style": "^0.4"
    },
    "autoload": {
        "psr-4": {
            "EzSystems\\EzRecommendationClientBundle\\": "src/bundle/",
            "EzSystems\\EzRecommendationClient\\": "src/lib/"
        }
    },
    "scripts": {
        "fix-cs": "@php ./vendor/bin/php-cs-fixer fix -v --show-progress=estimating"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
